# YBU 自动选课系统配置文件
# 复制此文件为 .env 并填写实际配置

# 用户凭据
YBU_USER=123xxxxx
YBU_PASS=********

# 浏览器设置
HEADLESS=true

# 验证码识别设置
CAPTCHA_MODE=ai
# CAPTCHA_MODE=manual  # 手动输入模式

# 验证码重试设置
CAPTCHA_MAX_RETRIES=3
CAPTCHA_MANUAL_TAKEOVER=true

# 代理设置（可选）
PROXY=

# 调试模式
DEBUG=false

# Web界面配置
WEB_HOST=0.0.0.0
WEB_PORT=3000
WEB_DEBUG=false 